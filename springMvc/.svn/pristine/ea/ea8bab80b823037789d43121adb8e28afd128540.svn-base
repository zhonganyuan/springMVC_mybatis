package com.wkb.core.exceptions;

/**
 * 权限信息异常
 * @author huanglt
 *
 */
public class InvalidAuthorityException extends Throwable {

	private static final long serialVersionUID = 1L;

	public InvalidAuthorityException(){
		super();
	}
	
	public InvalidAuthorityException(String mes){
		super(mes);
	}
	
	public InvalidAuthorityException(Throwable cause){
		super(cause);
	}
	
	public InvalidAuthorityException(String mes,Throwable cause){
		super(mes,cause);
	}
}
